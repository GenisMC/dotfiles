{
  "Svelte If": {
    "prefix": "sif",
    "body": [
      "{#if $1}",
      "$2",
      "{/if}"
    ],
    "description": "Svelte If"
  },
  "Svelte If Else": {
    "prefix": "sife",
    "body": [
      "{#if $1}",
      "$2",
      "{:else}",
      "$3",
      "{/if}"
    ],
    "description": "Svelte If Else"
  },
  "Svelte Each": {
    "prefix": "seach",
    "body": [
      "{#each $1 as $2}",
      "$3",
      "{/each}"
    ],
    "description": "Svelte Each"
  }
}
